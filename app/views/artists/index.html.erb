<p id="notice"><%= notice %></p>

<h1>Artists</h1>

<table>
  <thead>
    <tr>
      <th>Name artist</th>
      <th>Lastname artist</th>
      <th>Email artist</th>
      <th>City artist</th>
      <th>Country artist</th>
      <th>Cellphone artist</th>
      <th>Birthdate artist</th>
      <th>Linkgeneralphoto artist</th>
      <th>Nickname artist</th>
      <th>Linkvideosphotos</th>
      <th>Biopic artist</th>
      <th>Facebookuser artist</th>
      <th>Facebookfanpage artist</th>
      <th>Instagramuser artist</th>
      <th>Skills artist</th>
      <th>Work artist</th>
      <th>Address artist</th>
      <th>Rutinasnumber artist</th>
      <th>Stagetime artist</th>
      <th>Artistictime artist</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.name_artist %></td>
        <td><%= artist.lastname_artist %></td>
        <td><%= artist.email_artist %></td>
        <td><%= artist.city_artist %></td>
        <td><%= artist.country_artist %></td>
        <td><%= artist.cellphone_artist %></td>
        <td><%= artist.birthdate_artist %></td>
        <td><%= artist.linkgeneralphoto_artist %></td>
        <td><%= artist.nickname_artist %></td>
        <td><%= artist.linkvideosphotos %></td>
        <td><%= artist.biopic_artist %></td>
        <td><%= artist.facebookuser_artist %></td>
        <td><%= artist.facebookfanpage_artist %></td>
        <td><%= artist.instagramuser_artist %></td>
        <td><%= artist.skills_artist %></td>
        <td><%= artist.work_artist %></td>
        <td><%= artist.address_artist %></td>
        <td><%= artist.rutinasnumber_artist %></td>
        <td><%= artist.stagetime_artist %></td>
        <td><%= artist.artistictime_artist %></td>
        <td><%= link_to 'Show', artist %></td>
        <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
        <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

